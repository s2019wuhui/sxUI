{
  "name": "sx-ui",
  "version": "1.1.0",
  "description": "个人网演示组件库",
  "main": "dist/index.umd.js",
  "keywords": [
    "vue",
    "ui",
    "sx-ui2"
  ],
  "author": "sxjack",
  "files": [
    "dist",
    "components"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run dll && npm run cssGulp",
    "lint": "vue-cli-service lint",
    "dll": "webpack --config ./webpack.dll.js",
    "cssGulp": "npx gulp sass",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "gulp": "^4.0.2",
    "gulp-minify-css": "^1.2.4",
    "gulp-sass": "^5.1.0",
    "sx-ui2": "^1.1.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "6.0.0",
    "sass-loader": "^5.0.1",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.9.10",
    "webpack-cli": "3.3.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
